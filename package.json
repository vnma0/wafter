{
  "name": "wafter",
  "version": "1.20.0",
  "description": "Mirai's Backend",
  "main": "src/index.js",
  "bin": "src/index.js",
  "scripts": {
    "compile": "ncc build src/index.js -m -o build/",
    "build": "npm run compile && loose-envify build/index.js | terser -c --warn -o build/wafter.js",
    "start": "node src/index.js --unhandled-rejections=strict",
    "test": "mocha",
    "dev": "nodemon src/index.js",
    "pkg": "npm run build && cd build/ && nexe -i wafter.js -r \"public/**/*\" -o ../dist/wafter"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vnma0/wafter.git"
  },
  "author": "vnma0",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vnma0/wafter/issues"
  },
  "homepage": "https://github.com/vnma0/wafter#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^3.3.0",
    "dotenv": "^8.2.0",
    "enquirer": "^2.3.4",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "express-static-gzip": "^2.0.6",
    "filenamify": "^4.1.0",
    "helmet": "^3.22.0",
    "ip": "^1.1.5",
    "isbinaryfile": "^4.0.5",
    "memorystore": "^1.6.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "neat-csv": "^5.2.0",
    "nedb": "^1.8.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "rate-limiter-flexible": "^2.1.3",
    "uuid": "^7.0.2",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@zeit/ncc": "^0.22.0",
    "eslint": "^6.8.0",
    "loose-envify": "^1.4.0",
    "mocha": "^7.1.1",
    "nexe": "^3.3.2",
    "nodemon": "^2.0.2",
    "terser": "^4.6.7"
  }
}
